class Map:

    # def generate_rivers():
    # def generate_forest():

    def print_map(self):
        print("â¬›" * (self.w + 2))
        for row in self.cells:
            print("â¬›", end="")
            for cell in row:
                if cell == 0:
                    print('ğŸŸ©', end="")
                elif cell == 1:
                    print('ğŸŒ²', end="")
                elif cell == 2:
                    print("ğŸŒŠ", end="")
                elif cell == 3:
                    print("ğŸ¥", end="")
                elif cell == 4:
                    print("ğŸª", end="")
            print("â¬›")
        print("â¬›" * (self.w + 2))
    def check_bounds(self, x ,y):
        if (x < 0 or y < 0 or x >= self.h  or y >= self.w):
            return False
        return True


    def __int__(self, w, h):
        self.w = w
        self.h = h
        self.cells = [[0 for i in range(w)] for j in range(h)]


tmp = Map(10, 10)
tmp.cells[1][1] = 1
tmp.cells[2][2] = 2
tmp.cells[3][3] = 3
tmp.cells[4][4] = 4

if (tmp.check_bounds(20, 30)):
    print('YES')

tmp.print_map()
